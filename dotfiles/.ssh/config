Host github.com
Hostname github.com
User git
IdentitiesOnly yes
PreferredAuthentications publickey
IdentityFile ~/.ssh/id_rsa_pl

Host bitbucket.org
Hostname bitbucket.org
User git
IdentitiesOnly yes
PreferredAuthentications publickey
IdentityFile ~/.ssh/id_rsa

# DO droplets
Host do-ubuntu-01
Hostname do-ubuntu-01
User root
IdentitiesOnly yes
